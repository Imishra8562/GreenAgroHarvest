    <!-- Blog Start -->
    <section class="section-xl pbmit-column-three">
        <div class="container">
            <div class="position-relative">
                <div class="pbmit-heading-subheading animation-style4">
                    <h4 class="pbmit-subtitle">Latest Blog</h4>
                    <h2 class="pbmit-title">Latest posts & articles</h2>
                </div>
                <div class="blog-arrow swiper-btn-custom d-flex flex-row-reverse"></div>
            </div>
            <div class="swiper-slider" data-autoplay="false" data-arrows-class="blog-arrow" data-loop="true" data-dots="false" data-arrows="true" data-columns="3" data-margin="30" data-effect="slide">
                <div class="swiper-wrapper">

                    <!-- Slide1 -->
                    @if (Model.List_Blog_Obj != null && Model.List_Blog_Obj.Count > 0)
                    {
                        foreach (var items in Model.List_Blog_Obj)
                        {
                            <article class="pbmit-ele-blog pbmit-blog-style-1 swiper-slide">
                                <div class="post-item">
                                    <div class="pbminfotech-box-content">
                                        <div class="pbmit-featured-container">
                                            <div class="pbmit-featured-img-wrapper">
                                                <div class="pbmit-featured-wrapper">
                                                    @if (!string.IsNullOrEmpty(items.Blog_Image))
                                                    {
                                                        <img src="@Url.Content(items.Blog_Image)" class="img-fluid" alt="">
                                                    }
                                                    else
                                                    {
                                                        <img src="~/Content/images/homepage-1/blog/blog-image-01.jpg" class="img-fluid" alt="">
                                                    }
                                                </div>
                                            </div>
                                            <a class="pbmit-blog-btn" href="@Url.Action("BlogDetails", "home", new { Blog_Id = items.Blog_Id})">
                                                <span class="pbmit-button-icon-wrapper">
                                                    <span class="pbmit-button-icon">
                                                        <i class="pbmit-base-icon-black-arrow-1"></i>
                                                    </span>
                                                </span>
                                            </a>
                                            <div class="pbmit-meta-cat-wrapper pbmit-meta-line">
                                                <div class="pbmit-meta-category">
                                                    <a href="@Url.Action("BlogDetails", "home", new { Blog_Id = items.Blog_Id})" rel="category tag">Food Crops</a>
                                                </div>
                                            </div>
                                            <a class="pbmit-link" href="@Url.Action("BlogDetails", "home", new { Blog_Id = items.Blog_Id})"></a>
                                        </div>
                                        <div class="pbmit-category-date-wraper d-flex align-items-center">
                                            <div class="pbmit-meta-date-wrapper pbmit-meta-line">
                                                <div class="pbmit-meta-date">
                                                    <span class="pbmit-post-date">
                                                        <i class="pbmit-base-icon-calendar-3"></i>@items.Created_On.Value.ToString("dd/MM/yyyy")
                                                    </span>
                                                </div>
                                            </div>
                                            <div class="pbmit-meta-author pbmit-meta-line">
                                                <span class="pbmit-post-author">
                                                    <i class="pbmit-base-icon-user-3"></i>admin
                                                </span>
                                            </div>
                                        </div>
                                        <div class="pbmit-content-wrapper">
                                            <h3 class="pbmit-post-title">
                                                <a href="@Url.Action("BlogDetails", "home", new { Blog_Id = items.Blog_Id})">@items.Blog_Name</a></h3>

                                        </div>
                                    </div>
                                </div>
                            </article>
                        }
                    }

                </div>
            </div>
        </div>
    </section>
    <!-- Blog End -->

</div>

