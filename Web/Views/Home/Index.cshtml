    <!-- About Us Start -->
    <section>
        <div class="container">
            <div class="about-us-one-bg">
                <div class="row g-0">
                    <div class="col-md-12 col-xl-6"></div>
                    <div class="col-md-12 col-xl-6">
                        <div class="about-us-one-rightbox">
                            <div class="pbmit-heading-subheading animation-style4">
                                <h4 class="pbmit-subtitle">What We Do</h4>
                                <h2 class="pbmit-title">Healthy life with fresh products</h2>
                                <div class="pbmit-heading-desc">
                                    Embrace a healthy life with our fresh, organic products, grown sustainably to ensure quality, nutrition, and environmental care, supporting your well-being and a greener future for all.
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="pbminfotech-ele-fid-style-3">
                                        <div class="pbmit-fld-contents d-flex align-items-center">
                                            <div class="pbmit-circle-outer" data-digit="90" data-fill="#ffffff" data-emptyfill="" data-before="" data-after="<sup>%</sup>" data-thickness="3" data-size="127">
                                                <div class="pbmit-circle">
                                                    <div class="pbmit-fid-inner">
                                                        <span class="pbmit-fid-before"></span>
                                                        <span class="pbmit-number-rotate numinate" data-appear-animation="animateDigits" data-from="0" data-to="90" data-interval="5" data-before="" data-before-style="" data-after="" data-after-style="">90</span>
                                                        <span class="pbmit-fid"><sup>%</sup></span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="pbmit-fid-sub">
                                                <h3 class="pbmit-fid-title">Eco Farms Worldwide</h3>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 mt-md-0 mt-3">
                                    <div class="pbminfotech-ele-fid-style-3">
                                        <div class="pbmit-fld-contents d-flex align-items-center">
                                            <div class="pbmit-circle-outer" data-digit="78" data-fill="#ffffff" data-emptyfill="" data-before="" data-after="<sup>%</sup>" data-thickness="3" data-size="127">
                                                <div class="pbmit-circle">
                                                    <div class="pbmit-fid-inner">
                                                        <span class="pbmit-fid-before"></span>
                                                        <span class="pbmit-number-rotate numinate" data-appear-animation="animateDigits" data-from="0" data-to="78" data-interval="5" data-before="" data-before-style="" data-after="" data-after-style="">78</span>
                                                        <span class="pbmit-fid"><sup>%</sup></span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="pbmit-fid-sub">
                                                <h3 class="pbmit-fid-title">Special Equipment</h3>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- About Us End -->
    <!-- Ihbox Style 5 Start -->
    <section>
        <div class="section-xl container">
            <div class="ihbox-style-5-bg">
                <div class="pbmit-ihbox-style-5">
                    <div class="pbmit-ihbox-box">
                        <div class="pbmit-ihbox-icon">
                            <div class="pbmit-ihbox-icon-wrapper pbmit-icon-type-icon">
                                <i class="pbmit-agrimo-icon pbmit-agrimo-icon-plant"></i>
                            </div>
                        </div>
                        <div class="pbmit-ihbox-contents">
                            <h2 class="pbmit-element-title">We’re popular leader in<br> agriculture market globally</h2>
                        </div>
                        <div class="pbmit-ihbox-btn">
                            <a href="@Url.Action("Our_history","Home")">
                                <span class="pbmit-button-text">Discover more</span>
                                <span class="pbmit-button-icon-wrapper">
                                    <span class="pbmit-button-icon">
                                        <i class="pbmit-base-icon-black-arrow-1"></i>
                                    </span>
                                </span>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
