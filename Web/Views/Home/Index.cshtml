@model Web.Model.HomeModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="page-content">
    <div class="site-header header-style-1">
        <div class="pbmit-slider-area pbmit-slider-one">
            <div class="swiper-slider" data-autoplay="true" data-loop="true" data-dots="true" data-arrows="false" data-columns="1" data-margin="0" data-effect="fade">
                <div class="swiper-wrapper">
                    <!-- Slide1 -->
                    @if (Model.List_Banner_Obj != null && Model.List_Banner_Obj.Count > 0)
                    {
                        foreach (var items in Model.List_Banner_Obj)
                        {
                            <div class="swiper-slide">
                                <div class="pbmit-slider-item">
                                    @if (!string.IsNullOrEmpty(items.Banner_image))
                                    {
                                        <div class="pbmit-slider-bg" style="background-image: url(@items.Banner_image.Replace("~",".."));"></div>

                                    }
                                    else
                                    {
                                        <div class="pbmit-slider-bg" style="background-image: url(../../Content/images/banner-slider-img/demo01-slide-01.jpg);"></div>

                                    }
                                    <div class="container">
                                        <div class="row g-0">
                                            <div class="col-md-9 col-lg-7">
                                                <div class="pbmit-slider-content">
                                                    <h5 class="pbmit-sub-title transform-top transform-delay-1">Innovation Grows Here</h5>
                                                    <h2 class="pbmit-title transform-left transform-delay-2">@items.Banner_heading</h2>
                                                    <p class="pbmit-desc transform-left transform-delay-3">@items.Banner_description</p>
                                                    <div class="pbmit-button transform-bottom transform-delay-4">
                                                        <a class="pbmit-btn pbmit-btn-white" href="@Url.Action("Contact", "Home")">
                                                            <span class="pbmit-button-text">Contact Us </span>
                                                            <span class="pbmit-button-icon-wrapper">
                                                                <span class="pbmit-button-icon">
                                                                    <i class="pbmit-base-icon-black-arrow-1"></i>
                                                                </span>
                                                            </span>
                                                        </a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                    }
                    <!-- Slide2 -->
                </div>
            </div>
        </div>
    </div>
